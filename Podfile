# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'PollingApp' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
pod 'Firebase'
pod ‘Firebase/Core’
pod ‘Firebase/Database’
pod 'Firebase/Auth'
  # Pods for PollingApp

  target 'PollingAppTests' do
    inherit! :search_paths
	pod 'Firebase'
	pod ‘Firebase/Core’
	pod ‘Firebase/Database’
	pod 'Firebase/Auth'
    # Pods for testing
  end

  target 'PollingAppUITests' do
    inherit! :search_paths
	pod 'Firebase'
	pod ‘Firebase/Core’
	pod ‘Firebase/Database’
	pod 'Firebase/Auth'
    # Pods for testing
  end

end

